Tab2:CreateButton({
   Name = "Pubsmasher Script",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/TheRealAvrwm/Zephyr-V2/refs/heads/main/versions/pubsmashededition.lua", true))()
      Rayfield:Notify({
         Title = "Script Activated!",
         Content = "Thanks for using Pubsmasher Script",
         Duration = 5,
         Image = 4483362458,
      })
   end,
})

